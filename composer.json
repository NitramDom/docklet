{
    "name": "slopjong/docklet",
    "description": "Docker client. Simply a PHP docker client.",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-uri": "dev-master",
        "zendframework/zend-http": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.37"
    },
    "autoload": {
        "psr-0": {
            "Docklet\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/slopjong/Component_ZendUri"
        },
        {
            "type": "vcs",
            "url": "https://github.com/slopjong/Component_ZendHttp"
        }
    ]
}
